<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="attach">

	<!-- 쿼리문작성 -->
	<!-- <![CDATA[]]> -->
	
	<!-- 파일 업로드 -->
	<insert id="insertFile" parameterType="com.javaex.vo.AttachVo">
		<![CDATA[
			insert into attach
			value (null, #{orgName}, #{saveName}, #{filePath},#{fileSize}, null, #{userNo})
		]]>
	</insert>

	
	
</mapper>